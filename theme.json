        "caption": true,
        "background": true,
        "defaultDuotone": true,
        "custom": false,
        "palette": [
           {
                "color": "#000000",
                "name": "Black",
                "slug": "black"
            },
            {
                "color": "#D6C6B0",
                "name": "Sand",
                "slug": "sand"
            },
            {
                "color": "#EB3425",
                "name": "Heart",
                "slug": "heart"
            } 
        ],
        "gradients": [
            {
                "gradient": "linear-gradient(-225deg, #FBF1DA, #E6B980)",
                "name": "Arabian Quarter",
                "slug": "arabian-quarter"
            },
            {
                "gradient": "linear-gradient(-225deg, #5C6E83, #151B21)",
                "name": "Dusk",
                "slug": "dusk"
            },
            {
                "gradient": "linear-gradient(-225deg, #F96548, #990118)",
                "name": "Dubai Afternoon",
                "slug": "dubai-afternoon"
            }
        ]
    },
    "custom": {},
    "dimensions": {},
    "layout": {
        "contentSize": "",
        "wideSize": ""
