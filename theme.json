                "size": "2.827rem"
            },
            {
                "name": "Extra Extra Large",
                "slug": "xx-large",
                "size": "3.998rem"
            }
        ]
    },
    "useRootPaddingAwareAlignments": true
}, 
"styles":{
    "spacing": {
        "padding": {
            "top": "0",
            "bottom": "0",
            "left": "2rem",
            "right": "2rem"


        }
    },
    "typography": {
        "fontFamily": "var(--wp--preset--font-family--roc-grotesk-wide-x-light)"
    },
    
    "elements": {
        "h1": {
            "typography": {
                "fontSize": "var(--wp--preset--font-size--{x-large})"
            }
        },
        "h2": {
            "typography": {
                "fontSize": "var(--wp--preset--font-size--{large})"
            }
        }
    }
},
"templateParts": [],
"patterns": []
}
