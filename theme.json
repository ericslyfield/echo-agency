{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
	  "appearanceTools": true,
	  "blocks": {
		"core/image": {
		  "lightbox": {
			"allowEditing": true,
			"enabled": true
		  }
		},
		"core/buttons": {
		  "spacing": {
			"blockGap": true
		  }
		},
		"core/group": {
			"border": {
				"radius": true
			},
			"spacing": {
				"padding": true
			}
		},
		"core/column": {
			"border": {
				"radius": true
			},
			"spacing": {
				"padding": true
			}
		}
	  },
	  "color": {
		"background": true,
		"custom": false,
		"customGradient": true,
		"defaultDuotone": true,
		"defaultGradients": false,
		"defaultPalette": false,
		"palette": [
			{
				"color": "#eb3425",
				"name": "Primary",
				"slug": "primary"
			},
			{
				"color": "#000",
				"name": "Secondary",
				"slug": "secondary"
			},
			{
				"color": "#D6C6B0",
				"name": "Tertiary",
				"slug": "tertiary"
			},
			{
				"color": "#000",
				"name": "Black",
				"slug": "black"
			},
			{
				"color": "#fff",
				"name": "White",
				"slug": "white"
			},
			{
				"color": "#262626",
				"name": "Off-Black",
				"slug": "off-black"
			},
			{
				"color": "#efefef",
				"name": "Off-White",
				"slug": "off-white"
			},
			{
			  "color": "#151B21",
			  "name": "Rich Black",
			  "slug": "rich-black"
			},
			{
			  "color": "#D6C6B0",
			  "name": "Sand",
			  "slug": "sand"
			},
			{
			  "color": "#F4EDED",
			  "name": "Snow Day",
			  "slug": "snow-day"
			},
			{
			  "color": "#eb3425",
			  "name": "Echo Red",
			  "slug": "echo-red"
			}
		  ],
		"gradients": [
		  {
			"gradient": "linear-gradient(-225deg, #FBF1DA, #E5B16E)",
			"name": "Arabian Quarter",
			"slug": "arabian-quarter"
		  },
		  {
			"gradient": "linear-gradient(-225deg, #5C6E83, #151B21)",
			"name": "Dusk",
			"slug": "dusk"
		  },
		  {
			"gradient": "linear-gradient(-225deg, #f46e2b, #b10e03)",
			"name": "Dubai Afternoon",
			"slug": "dubai-afternoon"
		  }
		],
		"text": true
	  },
	  "layout": {
		"contentSize": "1080px",
		"wideSize": "1440px"
	  },
	  "lightbox": {
		"enabled": true
	  },
	  "spacing": {
		"blockGap": false,
		"customSpacingSize": true,
		"spacingScale": {
		  "increment": 1.414,
		  "mediumStep": 1.5,
		  "operator": "*",
		  "steps": 7,
		  "unit": "rem"
		}
	  },
	  "typography": {
		"customFontSize": true,
		"defaultFontSizes": false,
		"fluid": true,
		"fontFamilies": [
			{
			  "fontFamily": "\"Roc Grotesk Extra Wide\"",
			  "name": "Roc Grotesk Extra Wide",
			  "slug": "roc-grotesk",
			  "fontFace": [
				{
				  "fontFamily": "Roc Grotesk Extra Wide",
				  "fontStretch": "normal",
				  "fontStyle": "normal",
				  "fontWeight": "200",
				  "src": [
					"file:./assets/fonts/roc-grotesk-extra-wide-extra-light.otf"
				  ]
				},
				{
				  "fontFamily": "Roc Grotesk Extra Wide",
				  "fontStretch": "normal",
				  "fontStyle": "normal",
				  "fontWeight": "300",
				  "src": [
					"file:./assets/fonts/roc-grotesk-extra-wide-light.otf"
				  ]
				},
				{
				  "fontFamily": "Roc Grotesk Extra Wide",
				  "fontStretch": "normal",
				  "fontStyle": "normal",
				  "fontWeight": "500",
				  "src": [
					"file:./assets/fonts/roc-grotesk-extra-wide-medium.otf"
				  ]
				},
				{
				  "fontFamily": "Roc Grotesk Extra Wide",
				  "fontStretch": "normal",
				  "fontStyle": "normal",
				  "fontWeight": "700",
				  "src": [
					"file:./assets/fonts/roc-grotesk-extra-wide-bold.otf"
				  ]
				},
				{
				  "fontFamily": "Roc Grotesk Extra Wide",
				  "fontStretch": "normal",
				  "fontStyle": "normal",
				  "fontWeight": "800",
				  "src": [
					"file:./assets/fonts/roc-grotesk-extra-wide-extra-bold.otf"
				  ]
				}
			  ]
			}
		  ],
		"fontSizes": [
		  {
			"fluid": true,
			"name": "Small",
			"size": "20px",
			"slug": "small"
		  },
		  {
			"fluid": true,
			"name": "Medium",
			"size": "30px",
			"slug": "medium"
		  },
		  {
			"fluid": true,
			"name": "Large",
			"size": "40px",
			"slug": "large"
		  },
		  {
			"fluid": true,
			"name": "Extra Large",
			"size": "72px",
			"slug": "x-large"
		  },
		  {
			"fluid": true,
			"name": "Extra Extra Large",
			"size": "92px",
			"slug": "xx-large"
		  }
		],
		"letterSpacing": true
	  },
	  "useRootPaddingAwareAlignments": true
	},
	"styles": {
	  "color": {
		"background": "var(--wp--preset--color--echo-red)"
	  },
	  "elements": {
		"h1": {
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--x-large)"
		  }
		},
		"h2": {
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--large)"
		  }
		},
		"h3": {
			"typography": {
			  "fontSize": "var(--wp--preset--font-size--medium)"
			}
		  },
		"heading": {
		  "typography": {
			"fontFamily": "var(--wp--preset--font-family--roc-grotesk)",
			"fontStyle": "normal",
			"fontWeight": "200"
		  }
		},
		"link": {
		  ":hover": {
			"color": {
			  "text": "var(--wp--preset--color--sand)"
			}
		  },
		  "color": {
			"text": "var(--wp--preset--color--rich-black)"
		  },
		  "typography": {
			"fontFamily": "var(--wp--preset--font-family--global)"
		  }
		}
	  },
	  "spacing": {
		"margin": {
		  "bottom": "0",
		  "left": "0",
		  "right": "0",
		  "top": "0"
		},
		"padding": {
		  "bottom": "0",
		  "left": "0",
		  "right": "0",
		  "top": "0"
		}
	  },
	  "typography": {
		"fontFamily": "var(--wp--preset--font-family--global)"
	  }
	},
	"templateParts": [
	  {
		"area": "header",
		"name": "header",
		"title": "Header"
	  },
	  {
		"area": "main",
		"name": "card",
		"title": "Card"
	  }
	]
  }
  