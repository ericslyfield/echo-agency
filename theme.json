        "defaultFontSizes": false,
        "fontFamilies": [
            { 
                "fontFamily": "Roc Grotesk",
                "name": "Roc Grotesk Wide",
                "slug": "global",
                "fontFace": [
                    {
                    "fontFamily": "'Roc Grotesk', sans-serif",
                    "fontStretch": "normal",
                    "fontStyle": "normal",
                    "fontWeight": "200",
                    "src": [
                        "file:./assets/fonts/RocGrotesk-WideExtraLight.otf"
                    ]
                    },
                    {
                        "fontFamily": "'Roc Grotesk', sans-serif",
                        "fontStretch": "normal",
                        "fontStyle": "normal",
                        "fontWeight": "300",
                        "src": [
                            "file:./assets/fonts/RocGrotesk-WideLight.otf"
                        ]
                    },
                    {
                        "fontFamily": "'Roc Grotesk', sans-serif",
                        "fontStretch": "normal",
                        "fontStyle": "normal",
                        "fontWeight": "400",
                        "src": [
                            "file:./assets/fonts/RocGrotesk-WideMedium.otf"
                        ]
                    }
                ]
            }
        ],
        "fontSizes": [
            
            {
                "fluid": {

                    "min": "",
                    "max": ""
                    

